{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 1%",
          "uglify": true
        },
        "useBuiltIns": true
      }
    ],
    "react",
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-object-rest-spread",
    ["transform-runtime", {
      "polyfill": false,
    }],
    [
      "transform-class-properties",
      {
        "spec": false
      }
    ]
  ]
}
